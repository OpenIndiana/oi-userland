    value=pkg:/library/desktop/at-spi2-core@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary \
    value="The Assistive Technology Service Provider Interface (AT-SPI) Core"
set name=pkg.description \
    value="The Assistive Technology Service Provider Interface (AT-SPI) is a toolkit-neutral way of facilitating accessibility in applications, by using native accessibility APIs. AT-SPI can also be used for automated testing of user interfaces, with tools such as the Linux Desktop Testing Project and Dogtail. AT-SPI is currently supported by GTK+ 2, Java/Swing, the Mozilla suite, and StarOffice/OpenOffice.org. Developers, such as Trolltech, are developing support for AT-SPI using Trolltech's accessible toolkit library."
set name=com.oracle.info.description value=at-spi2-core
