T{
\fBJSON_STRING\fP
T}
_
T{
\fBJSON_INTEGER\fP
T}
_
T{
\fBJSON_REAL\fP
T}
_
T{
\fBJSON_TRUE\fP
T}
_
T{
\fBJSON_FALSE\fP
T}
_
