# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:2
msgid "<b>C_ustom:</b> Uses custom set of effects."
msgstr "<b>_Personalizar:</b> Utiliza un conjunto personalizado de efectos."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:4
msgid ""
"<b>E_xtra:</b> Provides more aesthetically pleasing set of effects. Requires "
"faster graphics-card."
msgstr "<b>E_xtra:</b> Proporciona un conjunto de efectos de aspecto más atractivo. Necesita una tarjeta gráfica más rápida."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:6
msgid ""
"<b>N_ormal:</b> Provides improved usability and good balance between "
"attractiveness and moderate performance-requirements."
msgstr "<b>N_ormal:</b> Brinda un uso mejorado y un buen equilibrio entre un aspecto atractivo y requisitos de rendimiento moderado."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:8
msgid ""
"<b>_None:</b> Provides a simple desktop environment without any effects."
msgstr "<b>_Ninguno:</b> Proporciona un entorno de escritorio sencillo y sin efectos."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:61
msgid "Visual Effects"
msgstr "Efectos visuales"

