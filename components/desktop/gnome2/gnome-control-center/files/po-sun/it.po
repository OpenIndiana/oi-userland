# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:2
msgid "<b>C_ustom:</b> Uses custom set of effects."
msgstr "<b>Personali_zzato:</b> utilizza un insieme di effetti personalizzato."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:4
msgid ""
"<b>E_xtra:</b> Provides more aesthetically pleasing set of effects. Requires "
"faster graphics-card."
msgstr "<b>E_xtra:</b> fornisce un insieme di effetti estetici più accattivanti. Richiede una scheda grafica più veloce."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:6
msgid ""
"<b>N_ormal:</b> Provides improved usability and good balance between "
"attractiveness and moderate performance-requirements."
msgstr "<b>N_ormale:</b> costituisce un buon compromesso tra estetica e prestazioni discrete e offre una maggiore facilità di utilizzo."

# SUN_BRANDING
#: ../capplets/appearance/data/appearance.ui.h:8
msgid ""
"<b>_None:</b> Provides a simple desktop environment without any effects."
msgstr "<b>_Nessuno:</b> fornisce un ambiente desktop semplice e privo di effetti."

# SUN_BRANDING
