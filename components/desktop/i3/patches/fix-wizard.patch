-bindcode $mod+40 exec --no-startup-id i3-dmenu-desktop
+#bindcode $mod+40 exec --no-startup-id i3-dmenu-desktop
 
 # change focus
 bindcode $mod+44 focus left
@@ -60,10 +60,10 @@
 bindcode $mod+47 focus right
 
