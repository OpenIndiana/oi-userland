set name=pkg.fmri \
    value=pkg:/library/python/net-snmp-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.human-version value=$(HUMAN_VERSION)
set name=pkg.summary value="The Net-SNMP - Python 3 bindings"
set name=com.oracle.info.description value="Python 3 bindings for Net-SNMP"
set name=info.classification \
    value=org.opensolaris.category.2008:Development/Python
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.arc-caseid value=LSARC/2008/355 value=PSARC/2014/169
