<transform file path=usr.*/man/.+ -> default mangler.man.stability uncommitted>
set name=pkg.fmri \
    value=pkg:/database/mysql-55/client@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="MySQL 5.5 Client Executables"
set name=com.oracle.info.description value="MySQL 5.5 Client Executables"
set name=info.classification \
    value="org.opensolaris.category.2008:Development/Databases"
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.arc-caseid value=PSARC/2013/333
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)

license mysql-55.license license=GPLv2

link path=usr/bin/mysql \
     target=../mysql/5.5/bin/mysql mediator=mysql mediator-version=5.5 pkg.linted.pkglint.dupaction010.2=true
link path=usr/bin/mysqldump \
     target=../mysql/5.5/bin/mysqldump mediator=mysql mediator-version=5.5
link path=usr/bin/mysqladmin \
     target=../mysql/5.5/bin/mysqladmin mediator=mysql mediator-version=5.5

link path=usr/share/man/man1/mysql.1 \
     target=../../../mysql/5.5/man/man1/mysql.1 mediator=mysql mediator-version=5.5 pkg.linted.pkglint.dupaction010.2=true
link path=usr/share/man/man1/mysqldump.1 \
     target=../../../mysql/5.5/man/man1/mysqldump.1 mediator=mysql mediator-version=5.5
link path=usr/share/man/man1/mysqladmin.1 \
     target=../../../mysql/5.5/man/man1/mysqladmin.1 mediator=mysql mediator-version=5.5

