<transform file path=usr.*/man/.+ -> default mangler.man.stability uncommitted>
set name=pkg.fmri \
    value=pkg:/database/mysql-57/client@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="MySQL 5.7 Client Executables"
set name=com.oracle.info.description value="MySQL 5.7 Client Executables"
set name=info.classification \
    value=org.opensolaris.category.2008:Development/Databases
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.arc-caseid value=PSARC/2015/501
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
#
link path=usr/bin/mysql target=../mysql/5.7/bin/mysql mediator=mysql \
    mediator-version=5.7
link path=usr/bin/mysqladmin target=../mysql/5.7/bin/mysqladmin mediator=mysql \
    mediator-version=5.7
link path=usr/bin/mysqldump target=../mysql/5.7/bin/mysqldump mediator=mysql \
    mediator-version=5.7
