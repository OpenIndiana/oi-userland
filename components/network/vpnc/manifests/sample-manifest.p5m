license $(COMPONENT_LICENSE_FILE) license='$(COMPONENT_LICENSE)'

file path=etc/vpnc/default.conf
file path=etc/vpnc/vpnc-script
file path=usr/bin/$(MACH64)/cisco-decrypt
file path=usr/bin/$(MACH64)/pcf2vpnc
file path=usr/bin/cisco-decrypt
file path=usr/bin/pcf2vpnc
file path=usr/sbin/$(MACH64)/vpnc
file path=usr/sbin/$(MACH64)/vpnc-disconnect
file path=usr/sbin/vpnc
file path=usr/sbin/vpnc-disconnect
file path=usr/share/doc/vpnc/COPYING
file path=usr/share/man/man1/cisco-decrypt.1
file path=usr/share/man/man1/pcf2vpnc.1
file path=usr/share/man/man1m/vpnc.1m
