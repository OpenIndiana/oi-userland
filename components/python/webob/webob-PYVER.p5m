set name=pkg.fmri \
    value=pkg:/library/python/webob-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="WSGI request and response objects"
set name=pkg.description \
    value="WebOb provides wrappers around the WSGI request environment, and an object to help create WSGI responses.  The objects map much of the specified behavior of HTTP, including header parsing, content negotiation, and correct handling of conditional and range requests.  This helps you create rich applications and valid middleware without knowing all the complexities of WSGI and HTTP."
set name=com.oracle.info.description value="the Python webob module"
set name=com.oracle.info.tpno value=14503
set name=info.classification \
    value=org.opensolaris.category.2008:Development/Python
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=info.upstream value="Pylons Project <ianb@colorstudy.com>"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.arc-caseid value=PSARC/2013/156
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
file path=usr/lib/python$(PYVER)/vendor-packages/WebOb-$(COMPONENT_VERSION)-py$(PYVER).egg-info/PKG-INFO
file path=usr/lib/python$(PYVER)/vendor-packages/WebOb-$(COMPONENT_VERSION)-py$(PYVER).egg-info/SOURCES.txt
file path=usr/lib/python$(PYVER)/vendor-packages/WebOb-$(COMPONENT_VERSION)-py$(PYVER).egg-info/dependency_links.txt
file path=usr/lib/python$(PYVER)/vendor-packages/WebOb-$(COMPONENT_VERSION)-py$(PYVER).egg-info/requires.txt
file path=usr/lib/python$(PYVER)/vendor-packages/WebOb-$(COMPONENT_VERSION)-py$(PYVER).egg-info/top_level.txt
file path=usr/lib/python$(PYVER)/vendor-packages/WebOb-$(COMPONENT_VERSION)-py$(PYVER).egg-info/zip-safe
file path=usr/lib/python$(PYVER)/vendor-packages/webob/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/acceptparse.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/byterange.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/cachecontrol.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/client.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/compat.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/cookies.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/datetime_utils.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/dec.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/descriptors.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/etag.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/exc.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/headers.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/multidict.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/request.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/response.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/static.py
file path=usr/lib/python$(PYVER)/vendor-packages/webob/util.py
license docs/license.txt license=MIT

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on paste; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/paste-$(PYV)

# force a dependency on the webob package
depend type=require \
    fmri=library/python/webob@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)

# force the rename with an optional dependency on the old name
depend type=optional \
    fmri=library/python-2/webob-$(PYV)@1.2.3,5.12-5.12.0.0.0.41.0
