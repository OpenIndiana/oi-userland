+	printf("PyFrameObject f_code: %d %d\n", offsetof(PyFrameObject, f_code),
+	    offsetof(PyFrameObject32, f_code));
+	printf("PyFrameObject f_lasti: %d %d\n", offsetof(PyFrameObject,
