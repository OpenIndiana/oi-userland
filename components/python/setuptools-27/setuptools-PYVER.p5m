# force the rename with an optional dependency on the old name
depend type=optional \
    fmri=library/python-2/setuptools-$(PYV)@0.6.11,5.11-2015.0.2.1

# force a dependency on the Python $(PYVER) runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on the setuptools package greater than the 2.7 version
depend type=optional \
    fmri=library/python/setuptools@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
