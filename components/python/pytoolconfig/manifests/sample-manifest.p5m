set name=pkg.fmri value=pkg:/$(COMPONENT_FMRI)-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.human-version value=$(HUMAN_VERSION)
set name=pkg.summary value="$(COMPONENT_SUMMARY)"
set name=info.classification value="$(COMPONENT_CLASSIFICATION)"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)

license $(COMPONENT_LICENSE_FILE) license='$(COMPONENT_LICENSE)'

file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig-$(HUMAN_VERSION).dist-info/METADATA
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig-$(HUMAN_VERSION).dist-info/WHEEL
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig-$(HUMAN_VERSION).dist-info/licenses/LICENSE
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/_version.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/documentation.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/fields.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/py.typed
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/pytoolconfig.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/sources/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/sources/ini.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/sources/pyproject.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/sources/pytool.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/sources/setup_cfg.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/sources/source.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/types.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/universal_config.py
file path=usr/lib/python$(PYVER)/vendor-packages/pytoolconfig/utils.py

# python modules are unusable without python runtime binary
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# Automatically generated dependencies based on distribution metadata
depend type=require fmri=pkg:/library/python/packaging-$(PYV)
depend type=require fmri=pkg:/library/python/tomli-$(PYV)
