set name=pkg.fmri \
    value=pkg:/library/python/debtcollector-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary \
    value="A collection of Python deprecation patterns and strategies"
set name=pkg.description \
    value="A collection of Python deprecation patterns and strategies that help you collect your technical debt in a non-destructive manner."
set name=com.oracle.info.description value="the Python debtcollector module"
set name=com.oracle.info.tpno value=$(TPNO)
set name=info.classification \
    value=org.opensolaris.category.2008:Development/Python
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=info.upstream value="OpenStack <openstack-dev@lists.openstack.org>"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.arc-caseid value=PSARC/2016/360
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
#
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/PKG-INFO
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/SOURCES.txt
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/dependency_links.txt
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/not-zip-safe
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/pbr.json
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/requires.txt
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector-$(COMPONENT_VERSION)-py$(PYVER).egg-info/top_level.txt
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/_utils.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/fixtures/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/fixtures/disable.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/moves.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/removals.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/renames.py
file path=usr/lib/python$(PYVER)/vendor-packages/debtcollector/updating.py
#
license LICENSE license="Apache v2.0"

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on the debtcollector package
depend type=require \
    fmri=library/python/debtcollector@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)

# force a dependency on funcsigs; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/funcsigs-$(PYV)

# force a dependency on pbr; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/pbr-$(PYV)

# force a dependency on six; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/six-$(PYV)

# force a dependency on wrapt; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/wrapt-$(PYV)
