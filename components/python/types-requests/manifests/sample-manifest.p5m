set name=pkg.fmri value=pkg:/$(COMPONENT_FMRI)-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.human-version value=$(HUMAN_VERSION)
set name=pkg.summary value="$(COMPONENT_SUMMARY)"
set name=info.classification value="$(COMPONENT_CLASSIFICATION)"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)

license $(COMPONENT_LICENSE_FILE) license='$(COMPONENT_LICENSE)'

file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/METADATA.toml
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/__init__.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/__version__.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/adapters.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/api.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/auth.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/certs.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/compat.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/cookies.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/exceptions.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/help.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/hooks.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/models.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/packages.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/sessions.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/status_codes.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/structures.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/requests-stubs/utils.pyi
file path=usr/lib/python$(PYVER)/vendor-packages/types_requests-$(HUMAN_VERSION)-py$(PYVER).egg-info/PKG-INFO
file path=usr/lib/python$(PYVER)/vendor-packages/types_requests-$(HUMAN_VERSION)-py$(PYVER).egg-info/SOURCES.txt
file path=usr/lib/python$(PYVER)/vendor-packages/types_requests-$(HUMAN_VERSION)-py$(PYVER).egg-info/dependency_links.txt
file path=usr/lib/python$(PYVER)/vendor-packages/types_requests-$(HUMAN_VERSION)-py$(PYVER).egg-info/requires.txt
file path=usr/lib/python$(PYVER)/vendor-packages/types_requests-$(HUMAN_VERSION)-py$(PYVER).egg-info/top_level.txt

# python modules are unusable without python runtime binary
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# Automatically generated dependencies based on distribution metadata
depend type=require fmri=pkg:/library/python/types-urllib3-$(PYV)
