set name=pkg.fmri \
    value=pkg:/library/python/rapidjson-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary \
    value="JSON (Java Script Object Notation) encoder/decoder for Python"
set name=com.oracle.info.description value="the JSON encoder/decoder for Python"
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
dir group=bin mode=0755 owner=root path=usr/lib/python3.5
dir group=bin mode=0755 owner=root path=usr/lib/python3.5/vendor-packages
dir group=bin mode=0755 owner=root path=usr/lib/python3.5/vendor-packages/python_rapidjson-$(COMPONENT_VERSION)-py3.5.egg-info
file group=bin mode=0644 owner=root path=usr/lib/python3.5/vendor-packages/python_rapidjson-$(COMPONENT_VERSION)-py3.5.egg-info/PKG-INFO
file group=bin mode=0644 owner=root path=usr/lib/python3.5/vendor-packages/python_rapidjson-$(COMPONENT_VERSION)-py3.5.egg-info/SOURCES.txt
file group=bin mode=0644 owner=root path=usr/lib/python3.5/vendor-packages/python_rapidjson-$(COMPONENT_VERSION)-py3.5.egg-info/dependency_links.txt
file group=bin mode=0644 owner=root path=usr/lib/python3.5/vendor-packages/python_rapidjson-$(COMPONENT_VERSION)-py3.5.egg-info/top_level.txt
file group=bin mode=0755 owner=root path=usr/lib/python3.5/vendor-packages/rapidjson.cpython-37m.so
license rapidjson.license license=MIT

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python3.5 \
    pkg.debug.depend.path=usr/bin
