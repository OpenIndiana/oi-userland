set name=pkg.fmri value=pkg:/$(COMPONENT_FMRI)-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.human-version value=$(HUMAN_VERSION)
set name=pkg.summary value="$(COMPONENT_SUMMARY)"
set name=info.classification value="$(COMPONENT_CLASSIFICATION)"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)

license $(COMPONENT_LICENSE_FILE) license='$(COMPONENT_LICENSE)'

file path=usr/lib/python$(PYVER)/vendor-packages/service_identity-$(HUMAN_VERSION).dist-info/AUTHORS.rst
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity-$(HUMAN_VERSION).dist-info/LICENSE
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity-$(HUMAN_VERSION).dist-info/METADATA
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity-$(HUMAN_VERSION).dist-info/WHEEL
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity-$(HUMAN_VERSION).dist-info/top_level.txt
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity/_common.py
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity/_compat.py
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity/cryptography.py
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity/exceptions.py
file path=usr/lib/python$(PYVER)/vendor-packages/service_identity/pyopenssl.py

# python modules are unusable without python runtime binary
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# Automatically generated dependencies based on distribution metadata
depend type=require fmri=pkg:/library/python/attrs-$(PYV)
depend type=require fmri=pkg:/library/python/cryptography-$(PYV)
depend type=require fmri=pkg:/library/python/pyasn1-$(PYV)
depend type=require fmri=pkg:/library/python/pyasn1-modules-$(PYV)
depend type=require fmri=pkg:/library/python/six-$(PYV)
