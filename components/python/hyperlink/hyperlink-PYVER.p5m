set name=pkg.fmri value=pkg:/$(COMPONENT_FMRI)-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="$(COMPONENT_SUMMARY)"
set name=pkg.description value="$(COMPONENT_DESCRIPTION)"
set name=info.classification value="$(COMPONENT_CLASSIFICATION)"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)

license $(COMPONENT_LICENSE_FILE) license='$(COMPONENT_LICENSE)'

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on idna; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/idna-$(PYV)

file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink-$(COMPONENT_VERSION)-py$(PYVER).egg-info/PKG-INFO
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink-$(COMPONENT_VERSION)-py$(PYVER).egg-info/SOURCES.txt
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink-$(COMPONENT_VERSION)-py$(PYVER).egg-info/dependency_links.txt
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink-$(COMPONENT_VERSION)-py$(PYVER).egg-info/not-zip-safe
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink-$(COMPONENT_VERSION)-py$(PYVER).egg-info/requires.txt
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink-$(COMPONENT_VERSION)-py$(PYVER).egg-info/top_level.txt
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/_url.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/__init__.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/common.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/test_common.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/test_decoded_url.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/test_parse.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/test_scheme_registration.py
file path=usr/lib/python$(PYVER)/vendor-packages/hyperlink/test/test_url.py
