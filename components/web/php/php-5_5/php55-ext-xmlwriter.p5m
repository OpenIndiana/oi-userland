set name=pkg.fmri value=pkg:/web/php-55/extension/php-xmlwriter@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="PHP Server 5.5 (xmlwriter extension)"
set name=info.upstream-url value="http://www.php.net"
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
set name=info.classification \
    value="org.opensolaris.category.2008:Development/PHP"

license php55.license license="PHP55 License"

dir path=/etc/php
dir path=/etc/php/5.5
dir path=/etc/php/5.5/conf.d
dir path=/usr/php
dir path=/usr/php/5.5
dir path=/usr/php/5.5/extensions
dir path=/usr/php/5.5/extensions/$(MACH64)
file files/xmlwriter.ini path=/etc/php/5.5/conf.d/xmlwriter.ini preserve=true
file path=/usr/php/5.5/extensions/$(MACH64)/xmlwriter.so
file path=/usr/php/5.5/extensions/xmlwriter.so
depend fmri=pkg:/web/php-55/php-common type=require
