set name=pkg.fmri value=pkg:/$(COMPONENT_FMRI)-$(PLV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.human-version value=$(HUMAN_VERSION)
set name=pkg.summary value="$(COMPONENT_SUMMARY)"
set name=info.classification value="$(COMPONENT_CLASSIFICATION)"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)

license $(COMPONENT_LICENSE_FILE) license='$(COMPONENT_LICENSE)'

file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::Encode.3perl
file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::Overview.3perl
file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::Punycode.3perl
file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::Punycode::PP.3perl
file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::Standards.3perl
file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::UTS46.3perl
file path=usr/perl5/$(PERLVER)/man/man3perl/Net::IDN::UTS46::_Mapping.3perl
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/Encode.pm
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/Overview.pod
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/Punycode.pm
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/Punycode/PP.pm
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/Standards.pod
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/UTS46.pm
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/Net/IDN/UTS46/_Mapping.pm
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/auto/Net/IDN/Punycode/Punycode.bs
file path=usr/perl5/vendor_perl/$(PERLVER)/$(PERL_ARCH)/auto/Net/IDN/Punycode/Punycode.so

# perl modules are unusable without perl runtime binary
depend type=require fmri=__TBD pkg.debug.depend.file=perl \
    pkg.debug.depend.path=usr/perl5/$(PERLVER)/bin

