set name=pkg.fmri value=pkg:/web/php-55/extension/php-iconv@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary value="PHP Server 5.5 (iconv extension)"
set name=info.upstream-url value="http://www.php.net"
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
set name=info.classification \
    value="org.opensolaris.category.2008:Development/PHP"

license php55.license license="PHP55 License"

dir path=/etc
dir path=/etc/php
dir path=/etc/php/5.5
dir path=/etc/php/5.5/conf.d
dir path=/usr/php/5.5
dir path=/usr/php/5.5/include
dir path=/usr/php/5.5/include/php
dir path=/usr/php/5.5/include/php/ext
dir path=/usr/php/5.5/include/php/ext/iconv
dir path=/usr/php/5.5/lib
dir path=/usr/php/5.5/extensions
dir path=/usr/php/5.5/extensions/$(MACH64)
file path=/usr/php/5.5/include/php/ext/iconv/php_have_bsd_iconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_have_glibc_iconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_have_ibm_iconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_have_iconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_have_libiconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_iconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_iconv_aliased_libiconv.h
file path=/usr/php/5.5/include/php/ext/iconv/php_iconv_supports_errno.h
file path=/usr/php/5.5/include/php/ext/iconv/php_php_iconv_h_path.h
file path=/usr/php/5.5/include/php/ext/iconv/php_php_iconv_impl.h
file files/iconv.ini path=/etc/php/5.5/conf.d/iconv.ini preserve=true
file path=/usr/php/5.5/extensions/$(MACH64)/iconv.so
file path=/usr/php/5.5/extensions/iconv.so
depend fmri=pkg:/web/php-55/php-common type=require
